turtle.forward(200)
turtle.left(144)

#145
turtle.home
turtle.clear

turtle.up()
for i in range(4):
  turtle.forward(10)
  turtle.left(90)
  turtle.forward(10)
  turtle.down()
  for i in range(4)
    turtle. forward(100)
    turtle.left(-90)
  turtle.up()
  turtle.left(90)
  turtle.forward(10)
  turtle.left(90)
  turtle.forward(10)
  turtle.left(180)
  